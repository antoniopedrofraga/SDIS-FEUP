package database;

import java.util.ArrayList;

public class ChunksList extends ArrayList<Integer> {

	private static final long serialVersionUID = 1L;

	public void addChunk(Integer number) {
		for (int i = 0; i < this.size(); i++) {
			if (number == this.get(i))
				return;
		}
		this.add(number);
	}
}
