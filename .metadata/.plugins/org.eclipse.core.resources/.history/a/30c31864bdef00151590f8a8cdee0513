package peers;

import java.net.MulticastSocket;

import exceptions.ArgsException;

public class Peer {
	private String serverId;
	
	private MulticastSocket mc;
	private int mcPort;
	
	private MulticastSocket mdb;
	private int mdbPort;
	
	private MulticastSocket mdr;
	private int mdrPort;
	
	public static void main(String[] args) throws ArgsException {
		if (args.length != 7)
			throw new ArgsException("peer <Server ID> <MC> <MC port> <MDB> <MDB port> <MDR> <MDR port>");
	}
}
