package database;

public class ChunkInfo {
	String fileId;
	int chunkNo;
	
	public ChunkInfo(String fileId, int chunkNo) {
		this.fileId = fileId;
		this.chunkNo = chunkNo;
	}

	@Override
	public boolean equals(Object obj) {
		return this.fileId == ((ChunkInfo)obj).fileId && 
				this.chunkNo == ((ChunkInfo)obj).chunkNo;
	}
	@Override
	public int hashCode() {
        int result = this.chunkNo;
        result = 31 * result + Integer.parseInt(fileId);
        return result;
    }
}
