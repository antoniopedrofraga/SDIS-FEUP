package channels;

import java.net.InetAddress;
import java.net.MulticastSocket;

import peers.Peer.MulticastThread;

public class MCChannel {
	private MulticastThread mcThread;
	private MulticastSocket mcSocket;
	private InetAddress mcAddress;
	private int mcPort;
	
	public MCChannel(MulticastThread mcThread, MulticastSocket mcSocket, InetAddress mcAddress, int mcPort) {
		this.mcThread = mcThread;
		this.mcSocket = mcSocket;
		this.mcAddress = mcAddress;
		this.mcPort = mcPort;
	}
}
